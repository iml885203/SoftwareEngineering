
<%
  var form = {
    action: '/project/create',
    method: 'POST',
    input: {
      showManager:{
        title: '專案管理員',
        type: 'text',
        value: req.session.passport.username,
        disabled: 'disabled',
      },
      manager:{
        type: 'hidden',
        value: req.session.passport.user,
      },
      name: {
        title: '名稱',
        placeholder: '專案名稱',
      },
    },
  }
%>
<% include ../partials/FormOpen %>
<div class="form-group">
  <label class="col-md-4 control-label">成員</label>
  <div class="col-md-8">
    <div class="js-select-members" data-endpoint="/api/user/getMemberList">
      <div id="hacker-list">
        <ul class="selected-list"></ul>
        <input class="search form-control" placeholder='搜尋' />
        <ul class="list"></ul>
      </div>
    </div>
  </div>
</div>

<div class="form-group">
  <label class="col-md-4 control-label">專案描述</label>
  <div class="col-md-8">
    <textarea name="info" cols="30" rows="10" class="form-control" placeholder='輸入描述'></textarea>
  </div>
</div>

<%
  var form = {
    submit: '送出',
  }
%>
<% include ../partials/FormClose %>
